{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "eslint:recommended", "prettier"],

  "plugins": [],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "consistent-return": "off",
    "no-console": "off",
    "no-use-before-define": "off",
    "func-names": "off"
  }
}
